# start up
/run/initialize
#
# open a file to store our histogram
/analysis/openFile pdd.root
#
# activate the pdd graph
#/graphActivator/activatePDDGraph true
/analysis/h1/setActivation 0
#
# run a simulation
/run/beamOn 10000000
#
# save to the file
/analysis/write
/analysis/closeFile
#
# reset the pdd graph
#/graphActivator/activatePDDGraph false
/analysis/h1/setActivation 0 false
